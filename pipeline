pipeline {
    agent any

    stages {
        stage('git push') {
            steps {
                git branch: 'J2EE', url: 'https://github.com/shashirajraja/onlinebookstore.git'
            }
        }
          
        stage('mvn build') {
            steps {
               sh'mvn clean package'
            }
        }
         
          stage('deploy') {
            steps {
              sh'ls'
            }
        }
      }


    }
